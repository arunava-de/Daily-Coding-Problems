#Given a stream of elements too large to store in memory, pick a random element from the stream with uniform probability.
import random

def select_random(x):
    res = 0
    count = 0
    count += 1

    if count==1:
        res = x
    else:
        i = random.randrange(count)
        if i==count-1:
            res = x
    
    return res

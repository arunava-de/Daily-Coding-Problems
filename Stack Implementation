class Stack:
    
    def __init__(self):
        self.l = []
    
    def isempty(self):
        if self.l==[]:
            return True
        else:
            return False
        
    def pop(self):
        if self.isempty():
            print("Stack is already empty")
        else:
            return self.l.pop()
    
    def push(self, v):
        self.l.append(v)
    
    def max(self):
        if self.isempty():
            return None
        else:
            return max(self.l)
        
